{
  "name": "react-notion-x",
  "version": "4.19.8",
  "description": "Fast and accurate React renderer for Notion.",
  "repository": "NotionX/react-notion-x",
  "author": "Travis Fischer <travis@transitivebullsh.it>",
  "license": "MIT",
  "main": "build/cjs/index.js",
  "module": "build/esm/index.js",
  "typings": "build/esm/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "requires": "./build/cjs/index.js",
      "types": "./build/esm/index.d.ts",
      "default": "./build/esm/index.js"
    },
    "./third-party/code": {
      "requires": "./build/cjs/third-party/code.js",
      "types": "./build/esm/third-party/code.d.ts",
      "default": "./build/esm/third-party/code.js"
    },
    "./third-party/collection": {
      "requires": "./build/cjs/third-party/collection.js",
      "types": "./build/esm/third-party/collection.d.ts",
      "default": "./build/esm/third-party/collection.js"
    },
    "./third-party/equation": {
      "requires": "./build/cjs/third-party/equation.js",
      "types": "./build/esm/third-party/equation.d.ts",
      "default": "./build/esm/third-party/equation.js"
    },
    "./third-party/modal": {
      "requires": "./build/cjs/third-party/modal.js",
      "types": "./build/esm/third-party/modal.d.ts",
      "default": "./build/esm/third-party/modal.js"
    },
    "./third-party/pdf": {
      "requires": "./build/cjs/third-party/pdf.js",
      "types": "./build/esm/third-party/pdf.d.ts",
      "default": "./build/esm/third-party/pdf.js"
    },
    "./third-party/next": {
      "requires": "./build/cjs/third-party/next.js",
      "types": "./build/esm/third-party/next.d.ts",
      "default": "./build/esm/third-party/next.js"
    },
    "./styles.css": "./src/styles.css",
    "./src/styles.css": "./src/styles.css"
  },
  "typesVersions": {
    "*": {
      ".": [
        "build/esm/index.d.ts"
      ],
      "third-party/code": [
        "build/esm/third-party/code.d.ts"
      ],
      "third-party/collection": [
        "build/esm/third-party/collection.d.ts"
      ],
      "third-party/equation": [
        "build/esm/third-party/equation.d.ts"
      ],
      "third-party/modal": [
        "build/esm/third-party/modal.d.ts"
      ],
      "third-party/pdf": [
        "build/esm/third-party/pdf.d.ts"
      ],
      "third-party/next": [
        "build/esm/third-party/next.d.ts"
      ]
    }
  },
  "engines": {
    "node": ">=12"
  },
  "dependencies": {
    "@matejmazur/react-katex": "^3.1.3",
    "date-fns": "^2.15.0",
    "format-number": "^3.0.0",
    "is-url-superb": "4",
    "katex": "^0.13.18",
    "lodash.throttle": "^4.1.1",
    "medium-zoom": "^1.0.6",
    "notion-types": "^4.19.8",
    "notion-utils": "^4.19.8",
    "prismjs": "^1.27.0",
    "rc-dropdown": "^3.1.2",
    "rc-menu": "^9.0.14",
    "react-hotkeys-hook": "^3.0.3",
    "react-image": "^4.0.3",
    "react-lazy-images": "^1.1.0",
    "react-modal": "^3.14.3",
    "react-pdf": "^5.7.1",
    "react-use": "^17.3.1"
  },
  "devDependencies": {
    "@types/lodash.throttle": "^4.1.6",
    "@types/node": "^16.11.2",
    "@types/react": "^17.0.31",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "peerDependencies": {
    "next": ">=10",
    "react": ">=16",
    "react-dom": ">=16"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    }
  }
}
